  },
  "license": "MIT",
  "author": "Michal Pietrzak",
  "repository": {
    "type": "git",
    "url": "https://github.com/pamigomp/angularjs-template.git"
  },
  "main": "server.js",
  "scripts": {
    "postinstall": "bower install",
    "prestart": "npm install",
    "start": "node server.js",
    "pretest": "npm install",
    "test": "grunt karma:unit",
    "coveralls": "cat ./coverage/*/lcov.info | ./node_modules/.bin/coveralls && rm -rf ./coverage",
    "test-single-run": "grunt karma:singleRun",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "grunt protractor"
  },
  "dependencies": {
    "bower": "^1.7.9",
    "express": "^4.16.2",
    "path": "^0.12.7",
    "python-shell": "^0.5.0",
    "socket.io": "^2.0.4",
    "socket.io-stream": "^0.9.1",
    "child_process": "^1.0.2",
    "http": "0.0.0"
  },
  "devDependencies": {
  },
  "engines": {
    "node": "^4.4.6",
    "npm": "^2.15.5"
  },
